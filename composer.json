{
	"name": "typo3-on-aws/aws-image-recognition",
	"type": "typo3-cms-extension",
	"description": "Integrates Amazon Web Service (AWS) to detect objects, scenes, faces, recognize celebrities in images uploaded at the TYPO3 backend",
	"homepage": "https://t3rrific.com/typo3-on-aws/",
	"license": "CC-BY-NC-SA-4.0",
	"keywords": [
		"TYPO3",
		"extension",
		"AWS",
		"Rekognition"
	],
	"authors": [
		{
			"name": "Michael Schams",
			"role": "Developer"
		},
		{
			"name": "Rico Neumann",
			"role": "Developer"
		}
	],
	"config": {
		"sort-packages": true
	},
	"require": {
		"typo3/cms-core": "^10",
		"aws/aws-sdk-php": "^3"
	},
	"autoload": {
		"psr-4": {
			"Typo3OnAws\\AwsImageRecognition\\": "Classes"
		}
	},
	"replace": {
		"typo3-ter/aws-image-recognition": "self.version"
	}
}
